<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wilsonxin.requestdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/b1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/b1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ButtonActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".ViewPagerActivity"></activity>
        <activity android:name=".CurrentLabelActivity"></activity>
        <activity android:name=".AnimationActivity"></activity>
        <activity android:name=".BottomNavigationBarActivity"></activity>
        <activity android:name=".CheckBoxActivity"></activity>
    </application>
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest>